{
  "name": "github-bot",
  "version": "1.0.0",
  "private": "true",
  "main": "index.js",
  "repository": "https://github.com/opensumi/github-bot.git",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "_build": "esno build.ts",
    "build": "run-s gen _build",
    "dev": "run-s gen dev:preview",
    "gen": "esno generate.ts",
    "dev:preview": "run-p dev:build dev:worker dev:proxy",
    "dev:build": "yarn _build --watch",
    "dev:worker": "wrangler dev",
    "dev:proxy": "esno dev.ts",
    "publish:local": "yarn build && wrangler publish",
    "lint": "eslint . && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix",
    "test": "jest --config jest.config.json --verbose"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.13.0",
    "@cloudflare/wrangler": "^1.19.12",
    "@octokit/types": "^6.35.0",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "dotenv": "^16.0.1",
    "edge-mock": "^0.0.15",
    "esbuild": "^0.14.45",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.16.3",
    "jest": "^28.1.1",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "smee-client": "^1.2.3",
    "string.prototype.replaceall": "^1.0.6",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@octokit/auth-app": "^3.6.1",
    "@octokit/auth-unauthenticated": "^2.1.0",
    "@octokit/core": "^3.6.0",
    "@octokit/oauth-app": "^3.6.2",
    "@octokit/plugin-paginate-rest": "^2.18.0",
    "@octokit/rest": "^18.12.0",
    "@octokit/webhooks": "^9.26.0",
    "lodash": "^4.17.21",
    "mri": "^1.2.0"
  }
}
