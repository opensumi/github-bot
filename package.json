{
  "name": "github-bot",
  "version": "1.0.0",
  "private": "true",
  "main": "index.js",
  "repository": "https://github.com/opensumi/github-bot.git",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "ts:check": "tsc -p ./tsconfig.build.json --noEmit",
    "prebuild": "yarn ts:check && yarn gen",
    "build": "tsx build.ts",
    "watch": "yarn build --watch",
    "dev": "yarn gen && yarn dev:preview",
    "dev:preview": "run-p dev:worker dev:proxy",
    "dev:worker": "wrangler dev",
    "dev:proxy": "tsx dev.ts",
    "gen": "tsx ./scripts/generate-wrangler-toml.ts",
    "prepublish:local": "yarn build",
    "publish:local": "wrangler publish",
    "lint": "eslint . && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix",
    "test": "jest --config jest.config.json --verbose",
    "extract-fixtures": "tsx scripts/extract-webhook-example.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@octokit/types": "^9.0.0",
    "@octokit/webhooks-examples": "^6.10.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/tough-cookie": "^4.0.2",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "axios": "^1.3.3",
    "axios-cookiejar-support": "^4.0.6",
    "dotenv": "^16.0.3",
    "edge-mock": "^0.0.15",
    "esbuild": "^0.17.5",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.4.1",
    "magic-string": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "smee-client": "^1.2.3",
    "string.prototype.replaceall": "^1.0.7",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@bytemain/openai-fetch": "^1.0.4",
    "@octokit/auth-app": "^4.0.9",
    "@octokit/auth-unauthenticated": "^3.0.4",
    "@octokit/core": "^4.2.0",
    "@octokit/oauth-app": "^4.2.0",
    "@octokit/plugin-paginate-rest": "^6.0.0",
    "@octokit/rest": "^19.0.7",
    "@octokit/webhooks": "^10.7.0",
    "buffer": "^6.0.3",
    "eventsource-parser": "^0.1.0",
    "hono": "^2.7.5",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-gfm": "^2.0.2",
    "mdast-util-gfm-autolink-literal": "^1.0.3",
    "mdast-util-to-markdown": "^1.5.0",
    "micromark-extension-gfm": "^2.0.1",
    "micromark-extension-gfm-autolink-literal": "^1.0.3",
    "mri": "^1.2.0",
    "p-timeout": "^6.0.0",
    "quick-lru": "^6.1.1",
    "tough-cookie": "^4.1.2",
    "tsx": "^3.12.2",
    "uuid": "^9.0.0",
    "wrangler": "^2.8.1"
  }
}
