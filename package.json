{
  "name": "github-bot",
  "version": "1.0.0",
  "private": "true",
  "main": "index.js",
  "repository": "https://github.com/opensumi/github-bot.git",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "_build": "esno build.ts",
    "build": "run-s gen _build",
    "dev": "run-s gen dev:preview",
    "gen": "esno generate.ts",
    "dev:preview": "run-p dev:build dev:worker dev:proxy",
    "dev:build": "yarn _build --watch",
    "dev:worker": "wrangler dev",
    "dev:proxy": "esno dev.ts",
    "publish:local": "yarn build && wrangler publish",
    "lint": "eslint . && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix",
    "test": "jest --config jest.config.json --verbose"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@cloudflare/wrangler": "^1.19.12",
    "@octokit/types": "^6.39.0",
    "@types/jest": "^28.1.5",
    "@types/lodash": "^4.14.182",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "dotenv": "^16.0.1",
    "edge-mock": "^0.0.15",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.16.3",
    "jest": "^28.1.3",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "smee-client": "^1.2.3",
    "string.prototype.replaceall": "^1.0.6",
    "ts-jest": "^28.0.6",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@octokit/auth-app": "^4.0.4",
    "@octokit/auth-unauthenticated": "^3.0.1",
    "@octokit/core": "^4.0.4",
    "@octokit/oauth-app": "^4.0.6",
    "@octokit/plugin-paginate-rest": "^3.0.0",
    "@octokit/rest": "^19.0.3",
    "@octokit/webhooks": "^10.0.8",
    "lodash": "^4.17.21",
    "mri": "^1.2.0"
  }
}
