{
  "name": "github-bot",
  "version": "1.0.0",
  "private": "true",
  "main": "index.js",
  "repository": "https://github.com/opensumi/github-bot.git",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "prebuild": "yarn gen",
    "build": "wrangler publish --dry-run --outdir=dist",
    "dev": "yarn gen && yarn dev:preview",
    "dev:preview": "run-p dev:worker dev:proxy",
    "dev:worker": "wrangler dev",
    "dev:proxy": "esno dev.ts",
    "gen": "esno ./scripts/generate-wrangler-toml.ts",
    "publish:local": "wrangler publish",
    "lint": "eslint . && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix",
    "test": "jest --config jest.config.json --verbose"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@octokit/types": "^7.5.0",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv": "^16.0.2",
    "edge-mock": "^0.0.15",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.16.3",
    "jest": "^29.0.3",
    "minimist": "^1.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "smee-client": "^1.2.3",
    "string.prototype.replaceall": "^1.0.6",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@octokit/auth-app": "^4.0.5",
    "@octokit/auth-unauthenticated": "^3.0.2",
    "@octokit/core": "^4.0.5",
    "@octokit/oauth-app": "^4.0.8",
    "@octokit/plugin-paginate-rest": "^4.3.0",
    "@octokit/rest": "^19.0.4",
    "@octokit/webhooks": "^10.1.5",
    "hono": "^2.1.4",
    "lodash": "^4.17.21",
    "mri": "^1.2.0",
    "toucan-js": "^2.6.1",
    "wrangler": "^2.1.4"
  }
}
