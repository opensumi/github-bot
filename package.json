{
  "name": "github-bot",
  "packageManager": "yarn@4.2.2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/opensumi/github-bot.git",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,css,json,md}'",
    "build": "hereby",
    "build:libs": "hereby",
    "build:cfworker": "hereby",
    "build:node": "hereby",
    "watch": "hereby",
    "watch:libs": "hereby",
    "dev": "hereby",
    "dev:node": "hereby",
    "dev:node:debug": "hereby",
    "dev:worker:preview": "hereby",
    "dev:worker": "hereby",
    "dev:proxy": "hereby",
    "gen": "hereby",
    "prepublish:local": "yarn build",
    "publish:local": "wrangler publish",
    "publish:libs": "lerna publish",
    "lint": "eslint . && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'",
    "lint:fix": "eslint . --fix && prettier . -w",
    "test": "jest --config jest.config.js --verbose",
    "test:cov": "yarn test --coverage",
    "extract-fixtures": "tsx scripts/extract-webhook-example.ts",
    "generate:json-schemas": "node scripts/generate-json-schemas-all.mjs",
    "postinstall": "yarn build:libs",
    "clean": "rm -rf libs/*/lib"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.1",
    "@octokit/app": "^14.0.0",
    "@octokit/core": "^5.0.0",
    "@octokit/rest": "^20.0.1",
    "@octokit/webhooks": "^12.0.3",
    "@opensumi/bot-commander": "workspace:^",
    "@opensumi/cfworker-builder": "workspace:^",
    "@opensumi/dingtalk-bot": "workspace:^",
    "@opensumi/ide-utils": "^3.1.1",
    "@opensumi/octo-service": "workspace:^",
    "@opensumi/workers-kv": "workspace:^",
    "@opensumi/workers-utils": "workspace:^",
    "chatgpt": "^5.0.9",
    "dayjs": "^1.11.10",
    "hono": "^4.3.9",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-gfm": "^3.0.0",
    "mdast-util-gfm-autolink-literal": "^2.0.0",
    "mdast-util-to-markdown": "^1.5.0",
    "micromark-extension-gfm": "^2.0.1",
    "micromark-extension-gfm-autolink-literal": "^2.0.0",
    "mnemonist": "^0.39.5",
    "mri": "^1.2.0",
    "openai-fetch": "^1.1.0",
    "tsx": "^3.12.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@octokit/types": "^11.1.0",
    "@octokit/webhooks-examples": "^7.3.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.21.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "execa": "^9.2.0",
    "hereby": "^1.8.9",
    "jest": "^29.4.1",
    "lefthook": "^1.3.8",
    "lerna": "^7.1.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "smee-client": "^1.2.3",
    "string.prototype.replaceall": "^1.0.7",
    "ts-jest": "^29.0.5",
    "ts-json-schema-generator": "^2.1.1",
    "typescript": "^5.4.5",
    "wrangler": "^2.8.1"
  }
}
